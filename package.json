  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@sindresorhus/slugify": "^1.1.0",
    "algoliasearch": "^4.6.0",
    "chalk": "^4.1.0",
    "date-fns": "^2.16.1",
    "directory-tree": "^2.2.5",
    "dotenv": "^8.2.0",
    "eleventy-plugin-embed-everything": "^1.9.3",
    "glob": "^7.1.6",
    "got": "^11.8.0",
    "html-minifier": "^4.0.0",
    "inquirer": "^7.3.3",
    "javascript-stringify": "^2.0.1",
    "js-beautify": "^1.13.0",
    "js-yaml": "^3.14.1",
    "markdown-it": "^12.0.2",
    "markdown-it-attribution": "^0.1.1",
    "markdown-it-prism": "^2.1.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "number-to-words": "^1.2.4",
    "nunjucks": "^3.2.2",
    "rimraf": "^3.0.2",
    "sharp": "^0.27.0",
    "smartquotes": "^2.3.1",
    "uuid": "^8.3.1"
  },
  "engines": {
    "node": ">=14.15.0 <15"
