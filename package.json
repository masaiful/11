{
  "name": "log-nikhil-io",
  "version": "1.0.0",
  "description": "Nikhil's Log",
  "main": ".eleventy.js",
  "scripts": {
    "bootstrap-misc": "npm-run-all make-misc-folders pull:misc",
    "make-misc-folders": "node _scripts/make-misc-folders.js",
    "make-misc-manifest": "node _scripts/make-misc-manifest.js",
    "pull:misc": "node _scripts/pull-misc.js",
    "build:misc": "node _scripts/build-misc.js",
    "push:misc": "node _scripts/push-misc.js",
    "organize:misc": "node _scripts/organize-misc.js",
    "borg": "npm-run-all organize:misc build:misc make-misc-manifest",
    "build:site": "npx eleventy",
    "clean": "rimraf _site",
    "compress:site": "node _scripts/compress-site.js",
    "push:site": "node _scripts/push-site.js",
    "build": "node _scripts/build.js",
    "push": "bash _scripts/push.sh",
    "update-algolia": "node _scripts/update-algolia.js",
    "new": "node _scripts/create",
    "serve": "npx eleventy --watch --serve --port 9000",
    "start": "node _scripts/start.js"
  },
